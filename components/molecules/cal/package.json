{
  "name": "@scbd/eco-molecule-cal",
  "version": "0.0.3",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "yarn build:lib  && yarn build:wca && yarn build:wc",
    "build:lib": "vue-cli-service build src/components/index.vue --target lib --name Cal --dest dist/vue",
    "build:wc": "vue-cli-service build src/components/index.vue --target wc --name Eco-Cal --dest dist/wc",
    "build:wca": "vue-cli-service build src/components/index.vue --target wc-async --name Eco-Cal --dest dist/wca",
    "lint": "vue-cli-service lint",
    "release": "yarn publish --access=public"
  },
  "main": "dist/vue/Cal.umd.min.js",
  "dependencies": {
    "@scbd/eco-molecule-add-to-calendar": "^0.0.6",
    "@scbd/ecosystem-style": "^0.0.2",
    "@vue/cli": "3.0.0-rc.3",
    "axios": "^0.18.0",
    "luxon": "^1.3.0",
    "normalize.css": "^8.0.0",
    "velocity-animate": "^1.5.1",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@scbd/ecosystem-icons": "^0.0.1",
    "@vue/cli-plugin-babel": "^3.0.0-rc.3",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.3",
    "@vue/cli-service": "^3.0.0-rc.3",
    "vue-template-compiler": "^2.5.16"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "files": [
    "*"
  ]
}
