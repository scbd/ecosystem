{
  "name": "@biodiversity/eco-molecule-carousel",
  "version": "0.0.0",
  "scripts": {
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "build": "yarn build:lib && yarn build:wc && yarn build:wca",
    "build:lib": "vue-cli-service build src/components/index.vue --target lib --name AddToCalendar",
    "build:wc": "vue-cli-service build src/components/index.vue --target wc --name Add-To-Calendar --dest ../wc/",
    "build:wca": "vue-cli-service build src/components/index.vue --target wc-async --name Add-To-Calendar --dest ../wca/",
    "release": "yarn publish --access=public"
  },
  "main": "dist/Carousel.umd.min.js",
  "dependencies": {
    "@biodiversity/ecosystem-style": "^0.0.9",
    "normalize.css": "^8.0.0",
    "vue": "^2.5.16"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.14",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.14",
    "@vue/cli-service": "^3.0.0-beta.14",
    "glob-all": "^3.1.0",
    "purgecss-webpack-plugin": "^1.1.0",
    "vue-template-compiler": "^2.5.16"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "description": "Vue component to display a carousel."
}
